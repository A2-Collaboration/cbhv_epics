##################################################################
#######################    eemem n and m values    ###############
##################################################################

record (ai, "$(P):BOX:$(BOXNO):Read$(LETTER)$(LEVELNO)_$(CHANNELNO)")
{
    field (DTYP, "stream")
    field (INP, "@$(PROTO) read_mnx$(CHANNELNO)($(LETTER),$(LEVELNO)) termBox$(BOXNO)")
    field (SCAN, "I/O Intr")
}

record (calcout, "$(P):BOX:$(BOXNO):Set$(LETTER)$(LEVELNO)")
{
    field (INPA, "0")
    field (INPB, "0")
    field (INPC, "0")
    field (INPD, "0")
    field (INPE, "0")
    field (INPF, "0")
    field (INPG, "0")
    field (INPH, "0")
    field (CALC, "0")
    field (VAL, "0")
    field (DTYP, "stream")
    field (OUT, "@$(PROTO) set_nmx($(LETTER),$(LEVELNO)) termBox$(BOXNO)")
    field (SCAN, "Passive")
#    field (PRIO, "HIGH")
}

record (stringout, "$(P):BOX:$(BOXNO):UnprotectEememCards")
{
    field (DTYP, "stream")
    field (OUT, "@$(PROTO) unprotect_eemem termBox$(BOXNO)")
    field (PRIO, "HIGH")
    field (FLNK, "$(P):BOX:$(BOXNO):SetEememStartCards")
}

record (stringout, "$(P):BOX:$(BOXNO):ProtectEememCards")
{
    field (DTYP, "stream")
    field (OUT, "@$(PROTO) protect_eemem termBox$(BOXNO)")
    field (PRIO, "HIGH")
    field (FLNK, "$(P):BOX:$(BOXNO):SetEememEndCards")
}

record (stringout, "$(P):BOX:$(BOXNO):ReloadEememCards")
{
    field (DTYP, "stream")
    field (OUT, "@$(PROTO) reload_eemem termBox$(BOXNO)")
    field (PRIO, "HIGH")
}

record (calcout, "$(P):BOX:$(BOXNO):SetEememStartCards")
{
    field (INPA, "1")
    field (CALC, "A")
    field (OUT, "$(P):SetEememStatus.A")
}

record (calcout, "$(P):BOX:$(BOXNO):SetEememEndCards")
{
    field (INPA, "0")
    field (CALC, "A")
    field (OUT, "$(P):SetEememStatus.A")
}

record (stringout, "$(P):BOX:$(BOXNO):PrintEememCards")
{
    field (DTYP, "stream")
    field (OUT, "@$(PROTO) eemem_print termBox$(BOXNO)")
    field (PRIO, "HIGH")
    field (PINI, "YES")
}

record (stringout, "$(P):BOX:$(BOXNO):SetCards")
{
    field (DTYP, "stream")
    field (OUT, "@$(PROTO) set_eemem_value(CARDS) termBox$(BOXNO)")
    field (PRIO, "HIGH")
}